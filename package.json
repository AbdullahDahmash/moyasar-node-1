  "devDependencies": {
    "babel-cli": "^6.8.0",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-stage-2": "^6.5.0",
    "babel-register": "^6.8.0",
    "babel-repl": "0.0.6",
    "mocha": "^2.4.5",
    "nodemon": "^1.9.2"
  },
  "dependencies": {
    "request-promise": "^3.0.0"
