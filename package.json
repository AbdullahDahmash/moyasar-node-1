  "devDependencies": {
    "babel-cli": "^6.8.0",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-stage-2": "^6.5.0",
    "babel-register": "^6.8.0",
    "mocha": "^2.4.5",
    "nock": "^8.1.0"
